{
  "manifest_version": 2,
  "version": "0.4.0",
  "name": "Rancher Lite",
  "description": "Manage multiple rancher servers.",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "icons": {
    "128": "logo.png"
  },
  "permissions": [
    "wss://*/",
    "http://*/",
    "https://*/",
    "notifications",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='"
}